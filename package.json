{
  "name": "gdj-alle-website",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint ."
