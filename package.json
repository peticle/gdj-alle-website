  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "animejs": "^3.2.2",
    "sanitize.css": "^10.0.0",
    "three": "^0.168.0",
    "vue": "^3.5.5",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/animejs": "^3.1.12",
    "@types/node": "^22.7.4",
    "@types/three": "^0.168.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.78.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.1",
    "vue-tsc": "^2.0.29"
  }
